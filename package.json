{
  "name": "sire-frontend",
  "version": "2.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build",
    "build-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "test": "ng test --code-coverage",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p src/tsconfig.app.json --theme postmark -n \"SIRE\" --unitTestCoverage coverage/coverage-summary.json",
    "compodoc:watch-serve": "npx compodoc -p src/tsconfig.app.json --theme postmark -n \"SIRE\" -w -s --unitTestCoverage coverage/coverage-summary.json",
    "preinstall": "npx npm-force-resolutions"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^5.1.0",
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/material-moment-adapter": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@azure/msal-angular": "^2.1.1",
    "@azure/msal-browser": "^2.22.0",
    "@grapecity/wijmo.all": "^5.20222.877",
    "@ng-bootstrap/ng-bootstrap": "^9.1.0",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
    "d3": "^7.3.0",
    "d3-sankey": "^0.12.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "ibmcloud-appid-js": "^1.0.1",
    "ini": "^1.3.8",
    "jquery": "^3.6.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "microsoft-adal-angular6": "^1.3.0",
    "moment": "^2.29.1",
    "msal": "^1.4.16",
    "ngx-json-viewer": "^2.4.0",
    "ngx-pagination": "^5.0.0",
    "ngx-powerbi": "^3.0.0",
    "powerbi-client": "^2.11.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.18",
    "@angular/cli": "~11.2.18",
    "@angular/compiler-cli": "~11.2.14",
    "@types/d3": "^7.1.0",
    "@types/d3-sankey": "^0.11.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^14.10.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^4.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-sabarivka-reporter": "^3.2.5",
    "karma-typescript": "^5.5.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.7.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "useTabs": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "proseWrap": "preserve"
  },
  "resolutions": {
    "tough-cookie": "^4.1.3",
    "semver": "^7.5.4",
    "request": "^2.88.2"
  }
}
